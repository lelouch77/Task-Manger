{{#each model as |task|}}
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">{{task.title}}</h3>
    </div>
    <div class="panel-body">
      <p>Time Spent: {{task.timespent}}<input type="checkbox"  disabled style="float:right;width:2%;" checked={{task.isCompleted}}/></p>
      <p>Time Spent: {{task.createdat}}</p>
      {{#link-to 'tasks.edit' task.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
     <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' task}}>Delete</button>
    </div>
  </div>
{{/each}}
